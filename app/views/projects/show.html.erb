<div class="container-fluid gray-contact">
  <div class="row">
    <div class="col-md-3 text-center projects-np">
      <%= link_to project_slug_path(@project.previous.slug) do %>
          <i class=" fa fa-angle-double-left fa-2x"></i> <h3 style="display: inline;">Previous Project</h3>
        <% end %>
      </div>
      <div class="col-md-6">
        <div class="placeholder"></div>
        
      </div>
      <div class="col-md-3 text-center projects-np">
        <%= link_to project_slug_path(@project.next.slug) do %>
          <h3 style="display: inline;">Next Project</h3> <i class=" fa fa-angle-double-right fa-2x"></i>
        <% end %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
        <h2><%= @project.title %></h2>
        <% if (user_signed_in?) %>
          <%= link_to "edit project",edit_project_path, class: 'btn btn-default' %>
        <% end %>
        <p class="blog-date"><i class="fa fa-calendar"></i> <%= localize(@project.created_at, :format => :long) %></p>
        <%= markdown(@project.description) %><br/>
        <%= link_to 'Back', :back %>
    </div>
  </div>
</div>